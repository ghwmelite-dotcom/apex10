{
  "name": "apex10-cryptodiscover",
  "version": "1.0.0",
  "description": "Edge-Native Crypto Asset Discovery Platform",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "wrangler deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply apex10-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply apex10-db --remote",
    "db:studio": "drizzle-kit studio",
    "db:seed": "wrangler d1 execute apex10-db --local --file=./db/seed.sql",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@react-three/drei": "^9.88.17",
    "@react-three/fiber": "^8.15.19",
    "@tanstack/react-query": "^5.60.0",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.9.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.36.0",
    "framer-motion": "^11.11.0",
    "hono": "^4.6.0",
    "howler": "^2.2.4",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.460.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.0",
    "recharts": "^3.7.0",
    "tailwind-merge": "^2.5.0",
    "three": "^0.160.0",
    "viem": "^2.44.1",
    "wagmi": "^3.3.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.0.0",
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "drizzle-kit": "^0.28.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.4.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-pwa": "^1.2.0",
    "wrangler": "^3.90.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
